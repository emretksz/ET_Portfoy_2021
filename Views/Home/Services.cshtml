@model List<ET_MyBlog_2021_V2.Models.Services>
@{
    ViewBag.Title = "Hizmetlerim";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@section Sliders2{
    @Html.Partial("_SlidersTwo", true)

}

<div class="container py-5">
    <div class="row text-center text-white">
        <div class="col-lg-8 mx-auto">
            <h1 class="display-4" style="font-family:'Times New Roman'">Hizmetlerim</h1>
            <hr />
            <br />

            @*<p class=bg-primary-dark>Şu anda hizmet verdiğim alanlar</p>*@
        </div>
    </div>
</div><!-- End -->


<div class="container">
    <div class="row text-center">

        <!-- Team item -->@foreach (var item in Model)
        {
            <div class="col-xl-3 col-sm-6 mb-5">
                <div class="bg-white rounded shadow-sm py-5 px-4">
                    <img src="@item.ImageURL" alt="" width="100" class="img-fluid rounded-circle mb-3 img-thumbnail shadow-sm">
                    <h5 class="mb-0">@item.ServiceTitle</h5><span class="small text-uppercase text-muted">@item.Description</span>
                    <ul class="social mb-0 list-inline mt-3">
                        <li class="list-inline-item"><a href="#" class="social-link"><i class="fa fa-facebook-f"></i></a></li>
                        <li class="list-inline-item"><a href="#" class="social-link"><i class="fa fa-twitter"></i></a></li>
                        <li class="list-inline-item"><a href="#" class="social-link"><i class="fa fa-instagram"></i></a></li>
                        <li class="list-inline-item"><a href="#" class="social-link"><i class="fa fa-linkedin"></i></a></li>
                    </ul>
                </div>
            </div><!-- End -->

        }
    </div>
</div>



<section class="section latest-blog bg-1">
    <div class="container">
        <div class="row text-center justify-content-center mb-1 ">
        
        </div>

   
    </div>
</section>

