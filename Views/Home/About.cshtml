@model List< ET_MyBlog_2021_V2.Models.Abouts>
@{
    ViewBag.Title = "Hakkımda";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Sliders2{
    @Html.Partial("_SlidersTwo",true)
    
    }

<section class="section about-2 position-relative">
    <div class="container">
        <div class="row">
            @foreach (var item in Model)
            {

            <div class="col-lg-6 col-md-6">
                <div class="about-item pr-3 mb-5 mb-lg-0">
                    @*<span class="h6 text-color">@item.Title</span>*@
                    <h2 class="mt-3 mb-4 position-relative content-title">@item.Title</h2>
                    <p class="mb-5">@Html.Raw(item.Description)</p>

                   
                </div>
            </div>
            <div class="col-lg-6 col-md-6">
                <div class="about-item-img">
                    <img src="@item.ImageURL" alt="" class="img-fluid">
                </div>
            </div>
            }
        </div>
    </div>
</section>

<section class="section latest-blog bg-2">
    <div class="container">
        <div class="row text-center justify-content-center mb-1 ">

        </div>


    </div>
</section>
<!-- Section About End -->

